<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<package xmlns="http://www.zend.com/server/deployment-descriptor/1.0" version="1.0">
    <name>ZendServerNagios Plugin</name>
    <summary>Zend Server Nagios plugin</summary>
    <description>A plugin for Nagios that allows to monitor Zend Server Events and notifications, cluster status and licence validity</description>
    <version>
        <release>1.0.0</release>
    </version>
    <eula>LICENSE.txt</eula>
    <appdir>src</appdir>
    <scriptsdir>scripts</scriptsdir>
    <healthcheck>index.php</healthcheck>
    <dependencies>
    <required>
      <php>
        <min>5.3</min>
      </php>
      <zendframework2>
        <min>2.0</min>
      </zendframework2>
    </required>
  </dependencies>
</package>
